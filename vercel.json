{
  "framework": "fastapi",
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": [
        "api/core/**",
        "api/models/**",
        "api/services/**",
        "api/utils/**",
        "assets/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
