{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.11",
      "maxDuration": 120,
      "memory": 1024,
      "includeFiles": "{api/core/**,api/models/**,api/services/**,api/utils/**,assets/**}"
    }
  },
  "routes": [
    {
      "src": "/api/((?!download-image(?:/|$)).*)",
      "dest": "/api/index.py"
    }
  ]
}
